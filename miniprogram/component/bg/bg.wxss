/* component/bg.wxss */
.bg {
  position: absolute;
  z-index: -12;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.bg1 {
  animation: fade 4000ms infinite;
}
.bg2 {
  animation: fade 4000ms 3000ms infinite;
}
.bg3 {
  animation: fade 4000ms 1000ms infinite;
}
.bg4 {
  animation: fade 4000ms 2000ms infinite;
}